{
  "name": "@cartesius/sdk",
  "version": "0.0.0-development",
  "description": "Cartesius.io API-Client for Node.js and the Browser",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cartesius-io/js-sdk.git"
  },
  "homepage": "https://cartesius.io/",
  "keywords": [
    "geocoding",
    "geosearch",
    "geoanalytics",
    "address autocomplete",
    "address search-as-you-type",
    "address earch",
    "place search",
    "cartesius api",
    "cartesius.io"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "node": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "build:release": "rm -fr dist/* && tsup src/index.ts --format esm,cjs --minify --dts src/index.ts",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "author": "Cartesius <info@cartesius.io>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "17.6.1",
    "@commitlint/config-conventional": "17.6.1",
    "@types/jest": "29.5.1",
    "@types/node": "18.15.13",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.1",
    "prettier": "2.8.7",
    "semantic-release": "^21.0.0",
    "ts-jest": "29.1.0",
    "tsup": "^6.6.3",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  },
  "dependencies": {
    "axios": "^1.3.4"
  }
}
